{
  "name": "ulbiproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env mode=development",
    "start:prod": "webpack serve --env mode=production",
    "build": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"**/*.{ts,tsx}\"",
    "styleLint": "npx stylelint \"**/*.scss\" --fix",
    "styleLint-check": "npx stylelint \"**/*.scss\"",
    "test-unit": "jest --config config/jest/jest.config.ts",
    "test-ui": "npx loki test",
    "test-ui:ok": "npx loki approve",
    "test-ui:ci": "concurrently --kill-others --success first \"npm run storybook\" \"npm run test-ui\" \"deelay\"",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "build-storybook": "storybook build",
    "visual:report": "npm-run-all visual:report:*",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
